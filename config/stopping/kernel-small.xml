<?xml version="1.0"?>
<config>
    <model>
        <S>100.0</S>
        <n>1</n>
        <delta>0.0</delta>
        <rho>0.1</rho>
        <sigma>0.05</sigma>
        <time_periods>5</time_periods>
        <reward_type>max-call</reward_type>
        <K>100.0</K>
        <r>0.0</r>
        <seed>123</seed>
    </model>
    <solve>
        <type>kernel</type>
        <sample_count>500</sample_count>
        <seed>123</seed>
        <gamma>2E-7</gamma>
        <kappa>2.</kappa>
        <bandwidth>1.0</bandwidth>
        <peak>1E3</peak>
    </solve>
    <eval>
        <sample_count>1000</sample_count>
        <seed>456</seed>
    </eval>
    <plot>
        <low-price>50.0</low-price>
        <high-price>150.0</high-price>
        <delta>1.0</delta>
        <filename>kernel-small.csv</filename>
    </plot>
</config>
