<?xml version="1.0"?>
<config>
    <model>
        <S>100.0</S>
        <n>1</n>
        <delta>0.0</delta>
        <rho>0.1</rho>
        <sigma>5</sigma>
        <time_periods>5</time_periods>
        <reward_type>max-call</reward_type>
        <K>100.0</K>
        <r>0.0</r>
        <seed>123</seed>
    </model>
    <solve>
        <type>kernel</type>
        <sample_count>20</sample_count>
        <seed>123</seed>
        <gamma>1E-10</gamma>
        <kappa>2.0</kappa>
        <bandwidth>1</bandwidth>
        <peak>1.0</peak>
    </solve>
    <eval>
        <sample_count>1000</sample_count>
        <seed>456</seed>
    </eval>
    <plot>
        <low-price>90.0</low-price>
        <high-price>110.0</high-price>
        <delta>2.</delta>
        <filename>kernel-small-gamma-1E-4.csv</filename>
    </plot>
</config>
